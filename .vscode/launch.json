    "version": "0.2.0",
    "configurations": [
        {
            "name": "Yandex Cloud Function: IoT protocol adapter",
            "type": "python",
            "request": "launch",
            "program": "iotadapter.py",
            "console": "integratedTerminal",
            "env": {
                "VERBOSE_LOG": "Talse",
                "DB_HOSTNAME": "my-host-name",
                "DB_PORT": "6432",
                "DB_NAME": "my-postgre-db-name",
